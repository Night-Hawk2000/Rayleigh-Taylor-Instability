FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases          (water glycerol);

water
{
    transportModel  Newtonian;
    nu              1e-06;
    rho             1000;
}

glycerol
{
    transportModel  Newtonian;
    nu              0.001119746233148;
    rho             1261;
}

sigma           0.07;


